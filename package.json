{
  "name": "metube",
  "version": "1.0.0",
  "description": "Databases 4620 Group Project",
  "main": "index.js",
  "scripts": {
    "build-scss": "node-sass view/scss -o public",
    "watch-scss": "npm run build-scss; while true; do node-sass --watch view/scss -o public; sleep 1; done",
    "build-web": "cd view; webpack;",
    "watch-web": "cd view; webpack --watch;",
    "test-server": "php -S 'localhost:8000'",
    "build": "npm run build-scss; npm run build-web",
    "watch": "concurrently -k -p '[{name}]' -n 'Sass, Webpack' -c 'yellow.bold, cyan.bold' 'npm run watch-scss' 'npm run watch-web'"
  },
  "repository": {
    "type": "git",
    "url": "buffet@buffet.cs.clemson.edu:acarrab/MeTube"
  },
  "author": "Lucas Durham, Daniel Pebbles, Angelo Carrabba",
  "license": "MIT",
  "dependencies": {
    "@types/react-router-dom": "^4.2.4",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "concurrently": "^3.5.1",
    "jquery": "^3.3.1",
    "material-ui": "^0.20.0",
    "material-ui-search-bar": "^0.4.1",
    "node-sass": "^4.7.2",
    "popper.js": "^1.12.9",
    "react": "^15.6.2",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^15.6.2",
    "react-form": "^3.4.4",
    "react-router-dom": "^4.2.2",
    "react-scrollbar": "^0.5.4",
    "sass": "^1.0.0-beta.5.2",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/material-ui": "^0.20.7",
    "@types/react": "^16.0.37",
    "@types/react-autosuggest": "^9.3.3",
    "@types/react-dom": "^16.0.4",
    "@types/react-form": "^2.16.0",
    "bootstrap-sass": "^3.0.0"
  }
}
