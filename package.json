{
  "name": "metube",
  "version": "1.0.0",
  "description": "Databases 4620 Group Project",
  "main": "index.js",
    "scripts": {
	"mod": "chmod o+r resources/*/**",
    "build-sass": "node-sass scss -o resources",
    "watch-sass": "node-sass --watch scss -o resources",
    "watch-web": "webpack --watch; npm run mod",
    "build-web": "webpack; npm run mod",
    "build": "npm run build-sass; npm run build-web",
    "watch": "concurrently -k -p '[{name}]' -n 'Sass, Webpack' -c 'yellow.bold, cyan.bold' 'npm run watch-sass' 'npm run watch-web'"
  },
  "repository": {
    "type": "git",
    "url": "buffet@buffet.cs.clemson.edu:acarrab/MeTube"
  },
  "author": "Lucas Durham, Daniel Pebbles, Angelo Carrabba",
  "license": "MIT",
  "dependencies": {
    "@types/react-router-dom": "^4.2.4",
    "bootstrap": "^4.0.0",
    "concurrently": "^3.5.1",
    "jquery": "^3.3.1",
    "material-ui": "^0.20.0",
    "material-ui-search-bar": "^0.4.1",
    "node-sass": "^4.7.2",
    "popper.js": "^1.12.9",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "sass": "^1.0.0-beta.5.2",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@types/material-ui": "^0.20.6",
    "@types/react": "^16.0.37",
      "@types/react-dom": "^16.0.4",
      "bootstrap-sass": "^3.0.0"
  }
}
